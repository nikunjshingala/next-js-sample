{
  "name": "pgforme-admin-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:qa\" \"npm run watch:css\"",
    "build": "next build",
    "start": "next start",
    "build:css": "npx tailwindcss -i ./styles/globals.css -o ./styles/output.css",
    "watch:css": "npx tailwindcss -i ./styles/globals.css -o ./styles/output.css --watch",
    "dev:qa": "env-cmd -f .env.qa next dev",
    "start:qa": "env-cmd -f .env.qa next start",
    "build:qa": "env-cmd -f .env.qa next build",
    "start:dev1": "env-cmd -f .env.dev1 next start",
    "build:dev1": "env-cmd -f .env.dev1 next build",
    "dev:develop": "env-cmd -f .env.develop next dev",
    "start:develop": "env-cmd -f .env.develop next start",
    "build:develop": "env-cmd -f .env.develop next build",
    "dev:prod": "env-cmd -f .env.production next dev",
    "start:prod": "env-cmd -f .env.production next start",
    "build:prod": "env-cmd -f .env.production next build",
    "lint": "eslint --cache --fix .",
    "prepare": "husky install",
    "pre-commit": "npm run lint"
  },
  "dependencies": {
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "axios": "^1.3.5",
    "cookies-next": "^2.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.3.0",
    "formik": "^2.2.9",
    "lodash-es": "^4.17.21",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1"
  }
}
